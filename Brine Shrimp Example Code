#import extensions 
%matplotlib inline   
import matplotlib as mat
import matplotlib.pyplot as plt
import pandas as pd
import numpy as np
import seaborn as sns

#read uploaded data
data=pd.read_csv("Reaction1,2,3 - Sheet1.csv")

#Create a violin plot
g=sns.violinplot(data,palette="Set2", cut=1.5)
g.set_ylabel("Reaction Time (ms)")

#Create a distribution graph with kernel density estimate 
d=sns.displot(data,palette="Set2",bins=15,kde="True")
d.set_axis_labels(x_var="Reaction Time (ms)")

#Create a box plot 
d=sns.boxplot(data,palette="Set2")
d.set_ylabel("Reaction Time (ms)")

#Kernel density plot
q=sns.kdeplot(
   data,
   fill=True, common_norm=False, palette="Set2",
   alpha=.5, linewidth=0,)
q.set_xlabel("Reaction Time (ms)")

